<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:b="clr-namespace:Microsoft.AspNetCore.Components.WebView.Maui;assembly=Microsoft.AspNetCore.Components.WebView.Maui"
             xmlns:mb="clr-namespace:MauiBundler.Bootstrap"
             xmlns:aa="clr-namespace:MauiBundler.Components"
             x:Class="MauiBundler.Components.MauiBundlerView">

    <aa:FilesBlazorWebView x:Name="mauiWebView" HostPage="wwwroot/index.html">
        <aa:FilesBlazorWebView.RootComponents>
            <b:RootComponent Selector="#_blazApp" ComponentType="{x:Type mb:Main}" />
        </aa:FilesBlazorWebView.RootComponents>
    </aa:FilesBlazorWebView>
</ContentPage>
